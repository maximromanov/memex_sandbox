@article{CampopianoLand2011,
  title = {Land {{Tax}} ʿalā L-Misāḥa and Muqāsama: {{Legal Theory}} and the {{Balance}} of {{Social Forces}} in {{Early Medieval Iraq}} (6th-8th {{Centuries C}}.{{E}}.)},
  shorttitle = {Land {{Tax}} ʿalā L-Misāḥa and Muqāsama},
  author = {Campopiano, Michele},
  date = {2011},
