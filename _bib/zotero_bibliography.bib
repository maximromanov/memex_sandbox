  author = {Simonsohn, Uriel},
  date = {2010},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {53},
  pages = {579--620},
  publisher = {{Brill}},
  issn = {0022-4995},
  abstract = {This paper examines the context in which church leaders in the regions of Mesopotamia and the Fertile Crescent, during the first few centuries after the Arab conquest, were objecting to the appeal of the their coreligionists to judicial authorities outside ecclesiastical control. Rather than assuming that from the outset of the Islamic conquest Muslim judges served as immediate judicial alternatives, the paper shows that, at least in the early Islamic period, church leaders were often aiming their exhortations towards Christians who sought the authority of other Christian figures from outside ecclesiastical jurisdiction. Cet article se penche sur le contexte dans lequel les chefs de l'Église, en Mésopotamie et dans le Croissant fertile des siècles suivant la conquête arabe, s'opposaient au recours de leurs coreligionnaires à des autorités judiciaires non contrôlées par l'Église. Au lieu de supposer que des juges musulmans servirent d'alternative judiciaire dès l'époque de la conquête, cet article montre qu'au début de la période islamique, au moins, les exhortations des chefs de l'Église visaient souvent des chrétiens qui s'en remettaient à l'autorité d'autres chrétiens, en dehors de la juridiction ecclésiastique.},
  eprint = {20789811},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/SimonsohnU/Simonsohn 2010 - The Christians Whose Force is.pdf},
  number = {4}
}

