@article{FleischerMediterranean2018,
  title = {A {{Mediterranean Apocalypse}}: {{Prophecies}} of {{Empire}} in the {{Fifteenth}} and {{Sixteenth Centuries}}},
  shorttitle = {A {{Mediterranean Apocalypse}}},
  author = {Fleischer, Cornell H.},
  date = {2018-03-14},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
