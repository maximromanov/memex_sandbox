@article{HasanProperty2018,
  title = {Property and {{Social Relations}} in {{Mughal India}}: {{Litigations}} and {{Disputes}} at the {{Qazi}}’s {{Court}} in {{Urban Localities}}, 17th-18th {{Centuries}}},
  shorttitle = {Property and {{Social Relations}} in {{Mughal India}}},
  author = {Hasan, Farhat},
  date = {2018-09-05},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
  pages = {851--877},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
  doi = {10.1163/15685209-12341470},
  abstract = {{$<$}section class="abstract"{$><$}h2 class="abstractTitle text-title my-1" id="d1598e2"{$>$}Abstract{$<$}/h2{$><$}p{$>$}Critiquing the commodity-centered frames of reference, this paper looks at property not within an economic logic, but as a set of practices that served to structure and reconfigure social relations. Based on a study of property documents and court papers, the essay argues that property was not simply an index of wealth, but a medium through which social relations were affirmed, reproduced and contested. Owing to the identification of property with the honor of families and caste groups, transactions in property were socially regulated activities that bore the imprint of local power relations. Property documents were imbued with a plethora of meanings, and this was because the scribal-literate tradition in Mughal India co-existed with an oral-performative culture. Writing was used by social actors in a wide variety of ways, and for different sets of objectives, sometimes to reinforce the social order, on other occasions to disrupt it.{$<$}/p{$><$}/section{$>$}},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/HasanF/Hasan 2018 - Property and Social Relations.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/7DZP365W/article-p851_3.html},
  langid = {english},
  number = {5-6}
}

