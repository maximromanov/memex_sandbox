@article{CoraFemale2018,
  title = {Female {{Labor}}, {{Merchant Capital}}, and {{Resilient Manufacturing}}: {{Rethinking Ottoman Armenian Communities}} through {{Labor}} and {{Business}}},
  shorttitle = {Female {{Labor}}, {{Merchant Capital}}, and {{Resilient Manufacturing}}},
  author = {Cora, Yasar Tolga},
  date = {2018-04-11},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
  pages = {361--395},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
