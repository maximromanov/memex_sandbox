@article{VatinCes2006,
  title = {Ces {{Messieurs}} de {{Galata}}. {{Note}} Sur {{Deux Rapports}} d'{{Expertise}} En {{Droit Maritime Rédigés}} En {{Août}} 1640 à {{Galata}} Au {{Profit}} d'un {{Capitaine Patmiote}}},
  author = {Vatin, Nicolas},
  date = {2006},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {49},
  pages = {48--67},
  publisher = {{Brill}},
  issn = {0022-4995},
  abstract = {The archives of Saint-John's Monastery in Patmos contain two documents dated to August 1640, concerning a dispute between two Patmiots about a question of partnership (muẓāraba). After a tentative explanation of the case itself from a practical as well as juridical point of view, this paper deals with the nature of the documents. Although they are expert's reports applying the şerīʿa, they are signed not by an ʿulemā, but by a sailor, " expert in Galata " (ehl-i ḥibre-i Ġalaṭa), speaking in the name of " the Muslims of Galata " (ehl-i ḳible-i Ġalaṭa): here is a very rare example of a written attestation of a non-judicial arbitration. Another remarkable fact is the presence in Galata of some kind of an office of experts specialised in maritime law, so well known that Patmiots doing business in Venice would consult them. It could be - but we can't be sure - an internal service of that guild of the captains of the White Sea mentioned by Evliyā Çelebi. Finally, we can stress that those Orthodox Greeks chose to have the Islamic Hanefi law applied to their case: neither the relative autonomy of the Patmiots nor their frequent contacts with Latins prevented them from feeling integrated into Ottoman society. /// Les archives du monastère de Saint-Jean à Patmos conservent deux documents d'août 1640 concernant un litige entre deux Patmiotes à propos d'une affaire de commandite (muẓāraba). Après avoir tenté de comprendre l'affaire elle-même, du point de vue pratique et juridique, cet article s'interroge sur la nature des documents. Il s'agit de rapports d'expertise discutant le cas en application de la şerīʿa, mais signés d'un marin " expert à Galata " (ehl-i ḥibre-i Ġalaṭa), parlant au nom des musulmans de Galata (ehl-i ḳible-i Ġalaṭa): nous avons là un exemple rarissime d'attestation écrite d'arbitrage non judiciaire. Le deuxième point notable est l'existence à Galata d'une sorte de cabinet d'experts en droit maritime assez reconnu pour que des Patmiotes faisant des affaires à Venise aillent le consulter. On est tenté de supposer, sans avoir de certitude, que ces experts formaient un organisme interne de la corporation des capitaines de la mer Blanche dont parle Evliyā Çelebi. On soulignera enfin que dans cette affaire non officielle, ces Grecs orthodoxes choisirent de se placer dans le cadre du droit hanéfite: la relative autonomie des Patmiotes et la fréquence de leurs relations avec les Latins ne les empêchaient pas de se considérer comme parfaitement intégrés à la société ottomane.},
  eprint = {25165128},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/VatinN/Vatin 2006 - Ces Messieurs de Galata.pdf},
  number = {1}
}

