@article{PianciolaCossacks2019,
  title = {Cossacks and {{Sturgeons}}: {{Fisheries}}, {{Colonization}}, and {{Science}} around the {{Aral Sea}} (1873-1906)},
  shorttitle = {Cossacks and {{Sturgeons}}},
