@article{LefevreMajalisi2012,
  title = {The {{Majālis}}-i {{Jahāngīrī}} (1608-11): {{Dialogue}} and {{Asiatic Otherness}} at the {{Mughal Court}}},
  shorttitle = {The {{Majālis}}-i {{Jahāngīrī}} (1608-11)},
  author = {Lefèvre, Corinne},
  date = {2012},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {55},
  pages = {255--286},
  publisher = {{BRILL}},
  issn = {0022-4995},
  abstract = {Building on the literary traditions of munãẓara (disputation) and malfūẓāt (teachings of a Sufi master), the Majālis-i Jahāngīrī (Assemblies of Jahāngīr) constitute a fundamentally dialogical work, in form as well as function. An account of the night-time sessions presided over by Emperor Jahāngīr from 1608 to 1611, this source highlights the Mughals' will to assert their power on a Eurasian scale and the central role played by Iran, Central Asia, and Hindustan in the elaboration of imperial ideology and identity. It thus opens a new window into the mental representations and hierarchies that underlay the much celebrated Mughal cosmopolitanism. S'ancrant dans la double tradition littéraire des munāẓara (disputation) et des malfuẓāt (conversations d'un maître soufi), les Majālis-i Jahāngīrī (Assemblées de Jahāngīr) constituent une œuvre fondamentalement dialogique, tant dans sa forme que dans son fonctionnement. Récit des séances nocturnes présidées par l'empereur Jahāngīr entre 1608 et 1611, ce texte donne à voir la volonté des Moghols d'affirmer leur pouvoir à une échelle eurasiatique et le rôle central joué par l'Iran, l'Asie centrale et l'Hindustan dans l'élaboration de l'idéologie et de l'identité impériale. Il permet, ce faisant, de mettre à jour les représentations mentales et les hiérarchies sous-tendant le cosmopolitisme tant célébré des Moghols.},
  eprint = {41725621},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/LefèvreC/Lefevre 2012 - The Majalis-i Jahangiri.pdf},
  number = {2/3}
}

@article{LefevreRecovering2007,
  title = {Recovering a {{Missing Voice}} from {{Mughal India}}: {{The Imperial Discourse}} of {{Jahāngīr}} (r. 1605-1627) in {{His Memoirs}}},
