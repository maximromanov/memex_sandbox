
@article{AbbottThat2019,
  title = {“{{In}} That {{One}} the {{Ālif}} Is {{Missing}}”: {{Eunuchs}} and the {{Politics}} of {{Masculinity}} in {{Early Colonial North India}}},
  shorttitle = {“{{In}} That {{One}} the {{Ālif}} Is {{Missing}}”},
  author = {Abbott, Nicholas},
  date = {2019-12-26},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {63},
  pages = {73--116},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
  doi = {10.1163/15685209-12341505},
  abstract = {{$<$}section class="abstract"{$><$}h2 class="abstractTitle text-title my-1" id="d311e2"{$>$}Abstract{$<$}/h2{$><$}p{$>$}Although ostensibly gendered as men and frequently maintaining independent, patriarchal households, enslaved eunuchs ({$<$}em{$>$}khwājasarā{$<$}/em{$>$}s) in pre- and early colonial regimes in South Asia were often mocked for their supposed effeminacy, bodily difference, and pretensions to normative masculinity. In the Mughal successor state of Awadh (1722-1856), such mockery grew more pronounced in the wake of growing financial demands from the British East India Company and attempts by eunuchs to alienate property with wills and testamentary bequests. Through examples of verbal derision directed at eunuchs, this essay shows that not only did ideas of normative masculinity serve as a vehicle for Awadh’s rulers to defend their sovereign authority from colonial encroachment, but that notions of normative manhood continued to inform eunuchs’ own self-perception into the nineteenth century.{$<$}/p{$><$}/section{$>$}},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/AbbottN/Abbott 2019 - “In that One the Ālif is.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/83M43T7T/article-p73_3.html},
  langid = {english},
  number = {1-2}
}

@article{AbdullahMandaean2018,
  title = {The {{Mandaean Community}} and {{Ottoman}}-{{British Rivalry}} in {{Late}} 19th-{{Century Iraq}}: {{The Curious Case}} of {{Shaykh Ṣaḥan}}},
  shorttitle = {The {{Mandaean Community}} and {{Ottoman}}-{{British Rivalry}} in {{Late}} 19th-{{Century Iraq}}},
  author = {Abdullah, Thabit A. J.},
  date = {2018-04-11},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {61},
  pages = {396--425},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
  doi = {10.1163/15685209-12341452},
  abstract = {{$<$}section class="abstract"{$><$}h2 class="abstractTitle text-title my-1" id="d862e2"{$>$}Abstract{$<$}/h2{$><$}p{$>$}In 1895, a Mandaean priest was captured near the town of Chabāyish in Iraq and brought to the jailhouse in Basra. Shaykh Ṣaḥan was accused of murdering his nephew and, more significantly, of supporting an Arab tribal rebellion against Ottoman authority. Using archival sources and Mandaean oral history, this article analyzes the case of Shaykh Ṣaḥan within the context of state centralization, Ottoman-British rivalry, and the internal conflicts among the Mandaeans. The case is significant because it sheds light on how large-scale transformations affected vulnerable minorities like the Mandaeans, and the way these communities struggled to survive in turbulent times.{$<$}/p{$><$}/section{$>$}},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/AbdullahT/Abdullah 2018 - The Mandaean Community and.pdf;/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroApp/storage/3Y53MUIS/article-p396_4.html},
  langid = {english},
  number = {3}
}

@article{AbdurasulovMaking2020,
  title = {Making {{Sense}} of {{Central Asia}} in {{Pre}}-{{Petrine Russia}}},
  author = {Abdurasulov, Ulfatbek},
  date = {2020-06-16},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {63},
  pages = {607--633},
  publisher = {{Brill}},
  issn = {1568-5209, 0022-4995},
  doi = {10.1163/15685209-12341516},
  abstract = {{$<$}section class="abstract"{$><$}h2 class="abstractTitle text-title my-1" id="d669e2"{$>$}Abstract{$<$}/h2{$><$}p{$>$}The existing historiography on liaisons between Russia and Central Asia in early modern period often tends to portray the cross-cultural diplomacy between the settings as an assemblage of sporadic, inefficient, clumsy encounters, full of diplomatic failures. Further to it, the dominant paradigm emphasizes cultural differences in the region, whereby any form of cross-cultural encounters was inevitably hampered by various confessional, religious and social borders. As a result, we tend to read every case of cross-cultural encounter between early modern Central Asia and Russia as a metaphor of cultural incommensurability. In the essay, I shall offer a close reading of two 17th-century Muscovite diplomatic missions to Central Asia as test cases with which to make sense of cultural encounters through the lens of individual actors. In doing so, I shall highlight the specific practices and strategies that allowed the diplomatic actors to play key roles as cultural mediators using their language skills, local knowledge and contact networks. In the broader sense, the essay set out to examine how can we problematize cross-cultural encounters between Central Asian principalities such as Khiva and Bukhara on the one hand, and Pre-Petrine Russia on the other: and to consider what we actually mean when we speak of early modern diplomacy in Central Eurasia.{$<$}/p{$><$}/section{$>$}},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/AbdurasulovU/Abdurasulov 2020 - Making Sense of Central Asia.pdf},
  langid = {english},
  number = {4}
}

