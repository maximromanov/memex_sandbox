@article{HeybergerPolemic2012,
  title = {Polemic {{Dialogues}} between {{Christians}} and {{Muslims}} in the {{Seventeenth Century}}},
  author = {Heyberger, Bernard},
  date = {2012},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {55},
  pages = {495--516},
  publisher = {{BRILL}},
  issn = {0022-4995},
  abstract = {Literary dialogues between Christians and Muslims can be traced back to the first centuries of Islam. Although the practice of disputation in the majlis (assembly) was no longer present in the Ottoman Empire, Catholic missionaries in the Middle East continued to write fictional dialogues in the seventeenth century. The authors we discuss here were convinced that it was possible to debate with Muslims, provided that the right type of interlocutor was chosen from among distinguished and educated men, with whom they shared a code of conduct and certain religious and intellectual concerns. This attitude may be the result of the typical Jesuit method of accommodatio and of their fieldwork experience in the Orient. Nevertheless, this positive approach was impeded by the impossibility that a Catholic priest could escape the culture of disputation and consider Islam as an ordinary subject of study. Le genre littéraire du dialogue entre chrétiens et musulmans remonte aux premiers siècles de l'Islam. Bien que le majlis de controverse ne se pratiquât plus dans l'empire ottoman au XVII e siècle, les missionnaires catholiques du Proche-Orient continuèrent à écrire des dialogues fictifs. Les auteurs évoqués ici sont convaincus qu'il est possible de discuter avec les musulmans, à condition de choisir le bon interlocuteur, parmi les hommes distingués et instruits, avec lesquels ils partagent un code de conduite et des préoccupations intellectuelles ou religieuses. Cette attitude pourrait être rapportée à l'expérience des missionnaires sur le terrain, et à la méthode jésuite de Y accommodatio. Néanmoins, cette approche positive de l'Islam est constamment contrariée par l'impossibilité pour un prêtre catholique d'échapper à la culture de controverse, et de considérer l'islam comme un sujet d'étude ordinaire.},
  eprint = {41725628},
  eprinttype = {jstor},
  file = {/Users/romanovienna/Dropbox/3.Reading/Zotero/ZoteroBooks/HeybergerB/Heyberger 2012 - Polemic Dialogues between.pdf},
  number = {2/3}
}

