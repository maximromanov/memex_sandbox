  langid = {english},
  number = {4}
}

@article{PickettArchetypical2019,
  title = {From the {{Archetypical Archive}} to {{Cultures}} of {{Documentation}}},
  author = {Pickett, James and Sartori, Paolo},
  date = {2019-11-12},
  journaltitle = {Journal of the Economic and Social History of the Orient},
  volume = {62},
