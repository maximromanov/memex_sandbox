@article{LycettFall2013,
  title = {The "{{Fall}}" of {{Vijayanagara Reconsidered}}: {{Political Destruction}} and {{Historical Construction}} in {{South Indian History}}},
  shorttitle = {The "{{Fall}}" of {{Vijayanagara Reconsidered}}},
  author = {Lycett, Mark T. and Morrison, Kathleen D.},
