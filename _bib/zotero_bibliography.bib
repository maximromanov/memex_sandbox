@article{OrbayFinancial2012a,
  title = {Financial {{Development}} of the {{Waqfs}} in {{Konya}} and the {{Agricultural Economy}} in the {{Central Anatolia}} ({{Late Sixteenth}}-{{Early Seventeenth Centuries}})},
