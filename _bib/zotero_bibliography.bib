@article{CohenPartnership2013a,
  title = {A {{Partnership Gone Bad}}: {{Business Relationships}} and the {{Evolving Law}} of the {{Cairo Geniza Period}}},
  shorttitle = {A {{Partnership Gone Bad}}},
  author = {Cohen, Mark R.},
  date = {2013},
