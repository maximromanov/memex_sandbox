@article{DemirciComplaints2003,
  title = {Complaints about {{Avâriz Assessment}} and {{Payment}} in the {{Avâriz}}-{{Tax System}}: {{An Aspect}} of the {{Relationship}} between {{Centre}} and {{Periphery}}. {{A Case Study}} of {{Kayseri}}, 1618-1700},
